{
  "name": "grade-git",
  "description": "A module that checks the current repo has good git repo hygiene",
  "version": "0.0.0",
  "author": "David Aghassi @Aghassi",
  "bin": {
    "grade-git": "./bin/run"
  },
  "bugs": "https://github.com/Aghassi/grade-git/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@oclif/dev-cli": "1.19.2",
    "eslint": "5.9.0",
    "eslint-config-oclif": "3.1.0",
    "globby": "9.1.0",
    "husky": "1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/Aghassi/grade-git",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "grade-git",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Aghassi/grade-git",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commitmsg": "commitlint -E GIT_PARAMS"
    }
  },
  "renovate": {
    "extends": [
      "config:base"
    ]
  }
}